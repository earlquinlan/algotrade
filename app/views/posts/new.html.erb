<!-- posts/new -->
<h1>Add Post</h1>

<%= form_for :post, url: posts_path do |f| %>
  <p>
  <%= f.label :title %><br>
  <%= f.text_field :title %>
  </p>

  <p>
  <%= f.label :body %><br>
  <%= f.text_field :body %>
  </p>

  <p>
  <%= f.submit "Add a Post!", class: "btn btn-primary" %>
  </p>

<% end %>
